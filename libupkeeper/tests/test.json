{
	"service run directory":"/var/run/upkeeper/buddy/",
	"service config directory":"/etc/upkeeper.d/",
	"controller socket":"/var/run/upkeeper/controller.sock",
	"default logging options": 
	{
		"stderr capture": true,
		"stdout capture": true,
		"dup stdout to file": true,
		"dup stderr to file": true,
		"dup stderr to syslog": false,
		"dup stdout to syslog": false,
	}
	"default logging path": "/var/log/upkeeper/",
	"default stdout log path": "/var/log/upkeeper/%s.out",
	"default stderr log path": "/var/log/upkeeper/%s.err",
}
